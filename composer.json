{
    "name": "slvler/whoami",
    "description": "ip-api.com API wrapper for Laravel",
    "keywords": [
        "ip-api",
        "laravel",
        "php"
    ],
    "homepage": "https://github.com/slvler/whoami",
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "slvler",
            "email": "slvler@proton.me"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/support": "^9.0 || ^10.0 "
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.6",
        "orchestra/testbench": "^5.0 || ^6.0 || ^7.0 || ^8.0 || ^9.0",
        "phpunit/phpunit": "^8.4 || ^9.3.3 || ^10.0"
    },
    "autoload": {
        "psr-4": {
            "Slvler\\Whoami\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Slvler\\Whoami\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit tests",
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Slvler\\Whoami\\WhoamiServiceProvider"
            ],
            "aliases": {
                "Whoami": "Slvler\\Whoami\\Facades\\WhoamiFacade"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
